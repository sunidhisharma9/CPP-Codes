//WORKING OF DIFFERENT DATA TYPES

#include <iostream>
#include <iomanip>
#include <limits>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
     int x;
    double y;
    string z;
    
    cin>>x >>y;
    cin.ignore();
    getline(cin,z);
    cout<<(x+i) <<endl <<fixed <<setprecision(1) << y+d <<endl;
    cout<<s<<z;
     return 0;
}
